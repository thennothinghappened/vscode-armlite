{
	"name": "vscode-armlite",
	"displayName": "vscode-armlite",
	"description": "armlite syntax highlighting and whatnot",
	"version": "1.0.0",
	"publisher": "orca",
	"icon": "images/icon.png",
	"author": {
		"name": "Dan Underwood"
	},
	"contributors": ["orca"],
	"categories": [
		"Programming Languages"
	],
	"engines": {
		"vscode": "^0.10.1"
	},
	"galleryBanner": {
		"color": "#0084A9",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/thennothinghappened/vscode-armlite.git"
	},
	"contributes": {
		"languages": [
			{
				"id": "armlite",
				"aliases": [
					"ARMLite",
					"armlite"
				],
				"extensions": [
					".asm",
					".armlite"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "armlite",
				"scopeName": "source.armlite",
				"path": "./syntaxes/armlite.tmlanguage.json",
				"embeddedLanguages": {
					"meta.embedded.markdown": "markdown"
				}
			}
		]
	}
}
